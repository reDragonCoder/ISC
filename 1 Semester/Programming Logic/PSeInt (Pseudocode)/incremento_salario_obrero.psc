Algoritmo incremento_salario_obrero
	definir salario_anterior, incremento, salario_nuevo como real
	imprimir "Ingrese su salario anterior: "
	leer salario_anterior
	incremento = salario_anterior*0.25
	salario_nuevo = salario_anterior+incremento
	imprimir "Su nuevo salario es: ", salario_nuevo
FinAlgoritmo
